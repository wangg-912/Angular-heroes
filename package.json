    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "lite-server": "^2.3.0",
    "lodash": "^4.17.4",
    "phantomjs-prebuilt": "^2.1.15",
    "protractor": "~5.2.0",
    "rollup": "^0.50.0",
    "rollup-plugin-commonjs": "^8.2.6",
